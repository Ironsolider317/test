// Create a torus
string $torus[] = `polyTorus -r 1 -sr 0.5 -tw 0 -sx 20 -sy 20 -ax 0 1 0 -cuv 1 -ch 1`;
select $torus;
move 0 1 0;
rename $torus[0] "ring1";


string $sphere[] = `polySphere -r 1 -sx 20 -sy 20 -ax 0 1 0 -cuv 2 -ch 1`;
select $sphere;
move 0 1 0;
rename $sphere[0] "ball"

// get everything selected
    string $sel[] = `ls -sl`;
    //create loop
for ($each in $sel)
{
    //do something
    select $each;
    move -r 0 1 0;
}


scale -r 3.038928 3.038928 3.038928 ;

// Create a variable staff with the values, pete, rob and jason
string $hi[] = {"pete","rob","jason"};
print $hi[1]; // print the second value

// cube test
string $happyCube[] = `polyCube -name "happyLittleCube"`;
print $happyCube[0];
string $cube = $happyCube[0];
move 0 1 0 -r $happyCube[0];

